.myclick :hover {
  font-weight: bolder;
}

.container {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  font: 25rpx/1.5 SimSun, "Product San", 'Noto Serif CJK SC', 'Source Han Serif SC', "Source Han Serif", source-han-serif-sc, "Noto SansCJK", "Helvetica Neue", Helvetica, "Heiti SC", "WenQuanYi Micro Hei";
  line-height: 2em;
}

page {
  width: 100%;
  height: 100%;
  background-repeat: repeat-y;
  background-size: contain;
  background-image: url('data:image/jpg;base64,');
}

scroll-view ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}